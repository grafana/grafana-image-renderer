{
  "service": {
    "host": "localhost",
    "port": 8081,
    "metrics": {
      "enabled": true,
      "collectDefaultMetrics": true,
      "requestDurationBuckets": [1, 5, 7, 9, 11, 13, 15, 20, 30]
    },
    "logging": {
      "level": "debug",
      "console": {
        "json": false,
        "colorize": true
      }
    },
    "fileRetention": {
      "enabled": true,
      "cronSchedule": "*/5 * * * * *",
      "retentionSeconds": 10
    }
  },
  "rendering": {
    "timezone": null,
    "chromeBin": null,
    "ignoresHttpsErrors": false,
    "timingMetrics": false,
    "mode": "default",
    "clustering": {
      "mode": "browser",
      "maxConcurrency": 5
    },
    "verboseLogging": false
  }
}