{
  "service": {
    "port": 8081,
    "metrics": {
      "enabled": true,
      "collectDefaultMetrics": true,
      "requestDurationBuckets": [1, 5, 7, 9, 11, 13, 15, 20, 30]
    }
  },
  "rendering": {
    "timezone": null,
    "chromeBin": null,
    "args": ["--no-sandbox", "--disable-gpu"],
    "ignoresHttpsErrors": false,
    "mode": "default",
    "clustering": {
      "mode": "browser",
      "maxConcurrency": 5,
      "timeout": 30
    },
    "timingMetrics": true
  }
}
